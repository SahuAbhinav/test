	<queryString>
		<![CDATA[SELECT inm.invoice_number, inm.invoice_date,party.party_name,city.city_name,itm.item_name
, SUM(ind.quantity) AS quantity, SUM(ind.primary_unit) AS primary_qty,ind.sales_rate, SUM(ind.item_value) AS item_value
, IFNULL(ind.batch_no,'') AS batch_no
, IFNULL(DATE_FORMAT(ind.expiry_date,'%m/%y'),'') AS exp_date
, SUM(IFNULL(ind.excise_amount,0)+IFNULL(ind.edu_cess_amount,0)+IFNULL(ind.h_edu_cess_amount,0)) AS excise_amount, SUM(ind.discount_amount) AS discount_amount
, SUM(IFNULL(ind.vat_amount,0)+IFNULL(ind.cst_amount,0)) AS tax_amount
, SUM(ind.net_amount) AS net_amount
,UCASE(unt.name) AS unit_name,UCASE(pck.name) AS packing
,IFNULL(itm.item_length,'') AS item_length
,IFNULL(itm.item_width,'') AS item_width
,IFNULL(itm.item_thikness,'') AS item_thikness
,IFNULL(itm.item_density,'') AS item_density
,gr.NAME AS Grade
,itm.item_sequence_no
FROM t_bill_mast AS inM,t_bill_detail AS inD,m_party AS party,m_city AS city
,m_item AS itm,m_item_category AS c,(SELECT masters_id,NAME FROM m_masters WHERE m_form_name='Measurement Unit' AND deleted_flag=0) AS unt
,(SELECT  masters_id ,NAME FROM m_masters WHERE m_form_name='Item Grade' AND deleted_flag=0) AS gr
, (SELECT masters_id,NAME FROM m_masters WHERE m_form_name='Pack Type' AND deleted_flag=0) AS pck
WHERE inm.invoice_number=ind.invoice_number AND inm.deleted_flag=0
AND itm.item_category_id=c.item_category_id AND c.deleted_flag=0
AND party.party_id = inm.party_id AND party.deleted_flag =0
AND inm.city_id = city.city_id AND city.deleted_flag = 0
AND ind.item_id=itm.item_id AND itm.deleted_flag=0
AND ind.primary_uom=unt.masters_id AND gr.masters_id=itm.item_grade_id
AND (c.ITEM_CATEGORY_NAME = $P{ItemCategoryPrompt} OR 1=$P{ItemCategoryNull_FlagPrompt})
AND(itm.item_name=$P{ItemNamePrompt} OR 1=$P{ItemNameNull_FlagPrompt})
AND(party.party_name=$P{PartyNamePrompt} OR 1=$P{PartyNameNull_FlagPrompt})
AND itm.pack_type_id=pck.masters_id
AND (itm.active_status=$P{Item_Status_Prompt} OR 1=$P{Item_StatusNull_FlagPrompt})
AND cast(inm.invoice_date as date) >= $P{FromDatePrompt}
AND cast(inm.invoice_date as date) <= $P{ToDatePrompt}
GROUP BY inm.invoice_number, inm.invoice_date,party.party_name,city.city_name,itm.item_name
, ind.sales_rate, batch_no, exp_date
ORDER BY itm.item_sequence_no,item_name,party_name]]>
