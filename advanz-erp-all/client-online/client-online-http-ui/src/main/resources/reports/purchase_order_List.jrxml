	<queryString>
		<![CDATA[SELECT pur_m.po_number,pur_m.po_date,prt.party_name
,pur_d.quantity,pur_d.purchase_rate
,ifnull((pur_d.quantity*pur_d.purchase_rate),0) AS item_value
, ((item_value*pur_d.excise_perc)/100) AS excise_amount
 ,((item_value*IFNULL(pur_d.vat_perc,0))/100) + ((item_value* IFNULL(pur_d.cst_perc,0))/100) AS tax_amount
,pur_d.discount_amount ,pur_d.net_amount, UPPER(IFNULL(itm.item_name,'')) AS item_name,UPPER(IFNULL(unt.name,'')) AS unit,UPPER(IFNULL(pck.name,'')) AS packing
FROM t_purchase_order_mast AS pur_m,t_purchase_order_detail AS pur_d
,m_party AS prt,m_item AS itm,m_masters AS unt,m_masters AS pck
WHERE pur_m.deleted_flag=0 AND pur_d.deleted_flag=0 AND pur_m.supplier_id =prt.party_id
AND pur_m.po_number = pur_d.po_number
AND prt.deleted_flag=0 AND pur_d.item_id=itm.item_id AND itm.deleted_flag=0
AND pur_d.measurement_unit_id=unt.masters_id AND unt.m_form_id=17 AND unt.deleted_flag=0
AND pck.deleted_flag=0 AND pck.m_form_id=15 AND itm.pack_type_id=pck.masters_id
AND pur_m.po_date>=$P{FromDatePrompt}
AND pur_m.po_date<=$P{ToDatePrompt}
AND(itm.item_name=$P{ItemNamePrompt} OR 1=$P{ItemNameNull_FlagPrompt})
AND(prt.party_name=$P{PartyNamePrompt} OR 1=$P{PartyNameNull_FlagPrompt})]]>
	</queryString>
