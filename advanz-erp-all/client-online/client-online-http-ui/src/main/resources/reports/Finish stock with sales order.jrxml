	<queryString>
		<![CDATA[SELECT o.ITEM_CATEGORY_NAME
, o.ITEM_NAME
, o.itemcode
, o.item_length
, o.item_width
, o.item_thikness
, o.item_density
, o.Grade
, o.UOM
, o.balance_qty
, IFNULL(o.sales_order_date,remove_null.sales_order_date) AS sales_order_date
, IFNULL(o.party_name, remove_null.party_name) AS party_name
, o.desire_delivery_date
, o.p_qty
, o.total_balance_qty
, o.item_sequence_no
FROM
(SELECT 1 AS map, c.ITEM_CATEGORY_NAME
, UPPER(itm.item_name) AS ITEM_NAME
, IFNULL(itm.itemcode,'') AS itemcode
,IFNULL(itm.item_length,'') AS item_length
,IFNULL(itm.item_width,'') AS item_width
,IFNULL(itm.item_thikness,'') AS item_thikness
,IFNULL(itm.item_density,'') AS item_density
,gr.NAME AS Grade
,unit.NAME AS UOM
,IFNULL(itm.opening_stock,0) + IFNULL(ledger_qty,0) AS balance_qty
,order_list.sales_order_date AS sales_order_date
,order_list.party_name AS party_name
,order_list.desire_delivery_date AS desire_delivery_date
,SUM((order_list.pending_qty)) AS p_qty
,(SELECT SUM(opening_qty) AS total_balance_qty
  FROM(SELECT itm.item_id
       ,IFNULL(itm.opening_stock,0) + IFNULL(ledger_qty,0) AS opening_qty
       FROM t_sales_order_mast AS sale_m,t_sales_order_detail AS sale_d,m_party AS prt,m_item_category AS c
       ,m_item AS itm LEFT JOIN (SELECT item_id,SUM(t_stock_ledger.quantity) AS ledger_qty
                                 FROM t_stock_ledger
                                 WHERE deleted_flag=0 AND transaction_date<=$P{ToDatePrompt}
                                 GROUP BY item_id) AS stock_ledger
                      ON itm.item_id=stock_ledger.item_id
       WHERE sale_m.deleted_flag=0 AND sale_d.deleted_flag=0 AND sale_m.party_id =prt.party_id
       AND sale_m.sales_order_number = sale_d.sales_order_number AND prt.deleted_flag=0 AND sale_d.item_id=itm.item_id
       AND itm.deleted_flag=0 AND itm.item_category_id=c.item_category_id AND c.deleted_flag=0
       AND sale_m.sales_order_date>=$P{FromDatePrompt}
       AND sale_m.sales_order_date<=$P{ToDatePrompt}
       AND (sale_m.desire_delivery_date=$P{DesireDelDatePrompt} OR 1=$P{DesireDelDateNull_FlagPrompt})
       AND (c.ITEM_CATEGORY_NAME = $P{ItemCategoryPrompt} OR 1=$P{ItemCategoryNull_FlagPrompt})
       AND(itm.item_name=$P{ItemNamePrompt} OR 1=$P{ItemNameNull_FlagPrompt})
       AND(prt.party_name=$P{PartyNamePrompt} OR 1=$P{PartyNameNull_FlagPrompt})
       AND (itm.active_status=$P{Item_Status_Prompt} OR 1=$P{Item_StatusNull_FlagPrompt})
       AND sale_d.pending_qty > 0
       GROUP BY itm.item_id
       ) AS bal_qty
  ) AS total_balance_qty
,item_sequence_no
FROM m_item_category AS c
, m_item AS itm LEFT JOIN (SELECT sale_m.desire_delivery_date, sales_order_date, prt.party_id
                           , sale_d.item_id, prt.party_name, sale_d.pending_qty
                           FROM t_sales_order_mast AS sale_m, t_sales_order_detail AS sale_d, m_party AS prt
                           WHERE sale_m.deleted_flag=0 AND sale_d.deleted_flag=0
                           AND sale_m.party_id =prt.party_id
                           AND sale_m.sales_order_number = sale_d.sales_order_number
                           AND prt.deleted_flag=0
                           AND sale_d.pending_qty > 0
                           AND sale_m.sales_order_date>=$P{FromDatePrompt}
                           AND sale_m.sales_order_date<=$P{ToDatePrompt}
                           AND(prt.party_name=$P{PartyNamePrompt} OR 1=$P{PartyNameNull_FlagPrompt})
                           AND (sale_m.desire_delivery_date=$P{DesireDelDatePrompt} OR 1=$P{DesireDelDateNull_FlagPrompt})
                           ) AS order_list
                ON itm.item_id = order_list.item_id
LEFT JOIN
(SELECT item_id,SUM(t_stock_ledger.quantity) AS ledger_qty
FROM t_stock_ledger
WHERE deleted_flag=0 AND transaction_date<=$P{ToDatePrompt}
GROUP BY item_id) AS stock_ledger
ON itm.item_id=stock_ledger.item_id
,(SELECT masters_id ,NAME FROM m_masters WHERE m_form_name='Item Grade' AND deleted_flag=0) AS gr
,(SELECT masters_id ,NAME FROM m_masters WHERE m_form_name='Measurement Unit' AND deleted_flag=0) AS unit
WHERE itm.deleted_flag=0 AND itm.item_category_id=c.item_category_id AND c.deleted_flag=0
AND gr.masters_id=itm.item_grade_id
AND unit.masters_id=itm.measurement_Unit_id
AND (c.ITEM_CATEGORY_NAME = $P{ItemCategoryPrompt} OR 1=$P{ItemCategoryNull_FlagPrompt})
AND(itm.item_name=$P{ItemNamePrompt} OR 1=$P{ItemNameNull_FlagPrompt})
AND (itm.active_status=$P{Item_Status_Prompt} OR 1=$P{Item_StatusNull_FlagPrompt})
GROUP BY itm.item_id,order_list.sales_order_date,order_list.party_id
ORDER BY itm.item_sequence_no) AS o
LEFT JOIN
(SELECT 1 AS map,sale_m.desire_delivery_date, sales_order_date, sale_d.item_id, prt.party_name
                           FROM t_sales_order_mast AS sale_m, t_sales_order_detail AS sale_d, m_party AS prt
                           WHERE sale_m.deleted_flag=0 AND sale_d.deleted_flag=0
                           AND sale_m.party_id =prt.party_id
                           AND sale_m.sales_order_number = sale_d.sales_order_number
                           AND prt.deleted_flag=0
                           AND sale_d.pending_qty > 0
                           AND sale_m.sales_order_date>=$P{FromDatePrompt}
                           AND sale_m.sales_order_date<=$P{ToDatePrompt}
                           LIMIT 1) AS remove_null
ON o.map = remove_null.map;]]>
	</queryString>
