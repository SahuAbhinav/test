	<queryString>
		<![CDATA[SELECT company.*, t_purchase_order_mast.po_number, t_purchase_order_mast.po_date, t_purchase_order_mast.indent_number, t_purchase_order_mast.indent_date
, t_purchase_order_mast.supplier_reference, t_purchase_order_mast.our_reference, t_purchase_order_mast.payment_terms, t_purchase_order_mast.desire_delivery_date
, t_purchase_order_mast.delivery_terms, m_city.city_name, m_transporter.transporter_name, m_item_group_flag.ITEM_GROUP_FLAG_Name, supplier.party_name, supplier.billing_address
, supplier.vat_no AS supplier_vatno , m_item.item_name, m_item.tariff_head_no AS tariff_head_no,t_purchase_order_detail.quantity
, t_purchase_order_detail.purchase_rate, t_purchase_order_mast.item_value AS sub_total_amt, t_purchase_order_mast.po_net_amount AS Amount
, m_masters.NAME, t_purchase_order_mast.discount_amount, InvCopy_tbl.InvCopy
, UPPER(t_purchase_order_mast.po_remark) AS remark , supplier.billing_address
FROM t_purchase_order_detail ,t_purchase_order_mast , m_city, m_transporter, m_item_group_flag, m_item, m_party AS supplier , (SELECT * FROM m_masters WHERE m_form_name='Measurement Unit' AND deleted_flag=0) AS m_masters ,
( SELECT (COMP_NAME) , COMP_ADD, VATNO, CSTNO, EXCISEECCNO, RANGE_ADD, DIVISION , regi_office_add ,
 IFNULL(commissionerate,'') AS COMMISIONRATE
FROM M_COMPANY
WHERE DELETED_FLAG=0 LIMIT 1) AS company, (SELECT 'PURCHASE ORDER' AS InvCopy) AS InvCopy_tbl
WHERE t_purchase_order_detail.deleted_flag=0 AND t_purchase_order_mast.deleted_flag=0
AND t_purchase_order_detail.po_number = t_purchase_order_mast.po_number
AND t_purchase_order_mast.city_id = m_city.city_id AND m_city.DELETED_FLAG=0
AND t_purchase_order_mast.transport_id = m_transporter.transporter_id AND m_transporter.DELETED_FLAG=0
AND t_purchase_order_mast.item_group_flag_id = m_item_group_flag.ITEM_GROUP_FLAG_ID
AND t_purchase_order_mast.supplier_id = supplier.party_id AND supplier.deleted_flag=0
AND m_item.deleted_flag=0
AND t_purchase_order_detail.item_id=m_item.item_id AND t_purchase_order_detail.measurement_Unit_id = m_masters.masters_id
AND (t_purchase_order_detail.po_number=$P{PONoPrompt} OR 1=$P{PONoNull_FlagPrompt})
ORDER BY 1]]>
	</queryString>
