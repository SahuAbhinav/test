	<parameter name="InvoiceNoPrompt" class="java.lang.String"/>
	<parameter name="InvoiceNoNull_FlagPrompt" class="java.lang.Integer" isForPrompting="false">
		<defaultValueExpression><![CDATA[$P{InvoiceNoPrompt}==null? new java.lang.Integer(1) :new java.lang.Integer(0)]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT company.*, t_bill_mast.invoice_number, t_bill_mast.invoice_date, t_bill_mast.sales_order_number, t_bill_mast.sales_order_date
, t_bill_mast.delivery_note_no, t_bill_mast.delivery_note_date, t_bill_mast.lr_no, t_bill_mast.lr_date ,t_bill_mast.despatch_through
, t_bill_mast.despatch_document_no, t_bill_mast.date_of_removal, t_bill_mast.time_of_removal, m_city.city_name, t_bill_mast.modified_date
, t_bill_mast.motor_vehicle_no, t_bill_mast.terms_of_payment, consignee.party_name AS consignee_party, consignee.vat_no AS consignee_vatno
, buyer.party_name AS buyer_party, buyer.vat_no AS buyer_vat, m_item.item_name, qty_per_packet, no_of_packet, t_bill_detail.quantity
, t_bill_detail.sales_rate , t_bill_detail.item_value AS Amount, m_masters.NAME, t_bill_detail.excise_perc, t_bill_mast.excise_duty_amount
, t_bill_mast.education_cess_perc, t_bill_mast.education_cess_amount, t_bill_mast.high_education_cess_perc, t_bill_mast.high_education_cess_amount
, (CASE WHEN consignee.invoice_type='saleswithInState' THEN t_bill_detail.vat_perc ELSE t_bill_detail.cst_perc END) AS VAT_CST_NO
, (CASE WHEN consignee.invoice_type='saleswithInState' THEN t_bill_mast.vat_amount ELSE t_bill_mast.cst_amount END) AS VAT_CST_AMT
, t_bill_mast.packing_forwarding, t_bill_mast.freight_amt, t_bill_mast.other_amount,t_bill_mast.bill_net_amount
FROM t_bill_detail , m_item, m_masters, t_bill_mast, m_city, m_party AS consignee, m_party AS buyer,
( SELECT COMP_NAME, COMP_ADD, VATNO, CSTNO, EXCISEECCNO, RANGE_ADD, DIVISION
, DIVISION AS COMMISIONRATE
FROM M_COMPANY
WHERE DELETED_FLAG=0 ) AS company
WHERE t_bill_detail.deleted_flag=0 AND m_item.deleted_flag=0 AND m_masters.deleted_flag=0
AND t_bill_detail.item_id=m_item.item_id AND t_bill_detail.measurement_Unit_id = m_masters.masters_id
AND t_bill_detail.invoice_number = t_bill_mast.invoice_number AND t_bill_mast.deleted_flag=0
AND t_bill_mast.city_id = m_city.city_id AND m_city.DELETED_FLAG=0
AND t_bill_mast.party_id = consignee.party_id AND consignee.deleted_flag=0
AND t_bill_mast.party_id = buyer.party_id
AND (t_bill_detail.invoice_number=$P{InvoiceNoPrompt} OR 1=$P{InvoiceNoNull_FlagPrompt})
-- and t_bill_detail.invoice_number='IN-1213-14'
-- group by t_bill_detail.invoice_number
ORDER BY 1]]>
	</queryString>
