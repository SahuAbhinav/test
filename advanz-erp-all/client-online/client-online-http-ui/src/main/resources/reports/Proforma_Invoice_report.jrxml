	<queryString>
		<![CDATA[SELECT inv_mast.* , mast.name AS m_form_name FROM
(SELECT company.*, t_proforma_mast.invoice_number, t_proforma_mast.invoice_date, t_proforma_mast.buyer_po_no , t_proforma_mast.buyer_po_date
, t_proforma_mast.delivery_note_no, t_proforma_mast.delivery_note_date, t_proforma_mast.lr_no, t_proforma_mast.lr_date ,t_proforma_mast.despatch_through
, t_proforma_mast.despatch_document_no, t_proforma_mast.date_of_removal, t_proforma_mast.time_of_removal, m_city.city_name, t_proforma_mast.modified_date
, t_proforma_mast.motor_vehicle_no, t_proforma_mast.terms_of_payment, consignee.party_name AS consignee_party, consignee.vat_no AS consignee_vatno
, buyer.party_name AS buyer_party, buyer.vat_no AS buyer_vat, m_item.invoice_name AS item_name
, IFNULL(UPPER(t_proforma_detail.packing_detail),'') AS description, m_item.tariff_head_no, qty_per_packet, no_of_packet, t_proforma_detail.quantity
, t_proforma_detail.sales_rate , t_proforma_detail.item_value AS Amount, t_proforma_mast.item_value AS sub_total_amount, t_proforma_mast.assessable_value, m_masters.NAME, t_proforma_detail.excise_perc, t_proforma_mast.excise_duty_amount
, t_proforma_mast.education_cess_perc, t_proforma_mast.education_cess_amount, t_proforma_mast.high_education_cess_perc, t_proforma_mast.high_education_cess_amount
, (IFNULL(t_proforma_detail.vat_perc,0) + IFNULL(t_proforma_detail.cst_perc,0)) AS VAT_CST_NO
, (IFNULL(t_proforma_mast.vat_amount,0) + IFNULL(t_proforma_mast.cst_amount,0)) AS VAT_CST_AMT
, t_proforma_mast.packing_forwarding, t_proforma_mast.freight_amt, t_proforma_mast.other_amount,t_proforma_mast.bill_net_amount
, InvCopy_tbl.InvCopy, IFNULL(t_proforma_mast.excise_duty_amount_inwords,'Nil') AS excise_duty_amount_inwords, IFNULL(t_proforma_mast.bill_net_amount_inwords,'Nil') AS bill_net_amount_inwords, t_proforma_mast.bill_net_amount_round_of
, t_proforma_mast.taxable_amount , t_proforma_mast.discount_amount, IFNULL(t_proforma_mast.high_education_cess_amount_inwords,'Nil') AS high_education_cess_amount_inwords
, IFNULL(t_proforma_mast.cess_amount_inwords,'Nil') AS cess_amount_inwords , UPPER(t_proforma_mast.bill_remark) AS bill_remark
, t_proforma_mast.form_type_id , consignee.billing_address AS consignee_address, buyer.billing_address AS buyer_address
, m_branch.duty_vide_notification AS duty_vide
, buyer.cst_no AS buyer_cst_no, consignee.cst_no AS consignee_cst_no , buyer.exciseeccno AS buyer_ecc_no
, consignee.exciseeccno AS consignee_ecc_no, t_proforma_detail.discount_amount AS item_discount_amount
, (CASE WHEN t_proforma_mast.sales_type='Sales with In State' THEN 'VAT' ELSE 'CST' END) AS vat_cst_tax_type
, m_state.state_name
FROM t_proforma_detail , m_item, (SELECT * FROM m_masters WHERE m_form_name='Measurement Unit' AND deleted_flag=0) AS m_masters, t_proforma_mast, m_city, m_party AS consignee, m_party AS buyer
, m_area, m_region, m_state ,
( SELECT COMP_NAME, COMP_ADD, VATNO, CSTNO, EXCISEECCNO, RANGE_ADD, DIVISION , regi_office_add ,
 IFNULL(commissionerate,'') AS COMMISIONRATE
FROM M_COMPANY
WHERE DELETED_FLAG=0 LIMIT 1) AS company,(SELECT '(NOT FOR CENVAT PURPOSE)' AS InvCopy) AS InvCopy_tbl , m_branch
WHERE t_proforma_detail.deleted_flag=0 AND m_item.deleted_flag=0 AND m_masters.deleted_flag=0
AND t_proforma_detail.item_id=m_item.item_id AND t_proforma_detail.measurement_Unit_id = m_masters.masters_id
AND t_proforma_detail.invoice_number = t_proforma_mast.invoice_number AND t_proforma_mast.deleted_flag=0
AND consignee.billing_city_id = m_city.city_id AND m_city.DELETED_FLAG=0 AND consignee.deleted_flag=0
AND m_area.deleted_flag=0 AND m_region.deleted_flag=0 AND m_state.deleted_flag=0
AND m_city.area_id=m_area.area_id AND m_area.region_id=m_region.region_id AND m_region.state_id=m_state.state_id
AND t_proforma_mast.party_id = buyer.party_id AND buyer.deleted_flag=0
AND t_proforma_mast.consignee_id = consignee.party_id
AND m_branch.DELETED_FLAG=0 AND t_proforma_mast.branch_id=m_branch.Branch_ID
-- AND t_proforma_mast.invoice_number='PI/13-14/351'
AND (t_proforma_detail.invoice_number=$P{InvoiceNoPrompt} OR 1=$P{InvoiceNoNull_FlagPrompt})
ORDER BY InvCopy_tbl.InvCopy, t_proforma_detail.sno ) AS inv_mast
LEFT JOIN (SELECT * FROM m_masters WHERE m_form_name='VAT Form Type' AND deleted_flag=0) AS mast
ON inv_mast.form_type_id=mast.masters_id]]>
	</queryString>
