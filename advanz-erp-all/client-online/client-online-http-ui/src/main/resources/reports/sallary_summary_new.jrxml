	<parameter name="YearPrompt" class="java.lang.String"/>
	<parameter name="SingleMandatoryMonthPrompt" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT a.sno,a.salary_head_id
,a.salary_head_name,a.head_amount,a.employee_id,a.Emp_name
,b.employee_id
,b.monthYear
,b.closing_leave_balance,b.opening_leave_balance
,b.availed_leave
,b.absent_days,b.extra_work_day
,b.hollyd_weekOff,b.loss_of_pay
,b.payable_day,b.present_days
,b.working_days,b.total_day_in_month
,b.employee_code,b.join_date,b.increment_date,b.opening_cl_balance
,b.designation
FROM
(SELECT 1 AS sno
,IF(m_salary_head.head_type='E',m_salary_head.salary_head_sequence+100,m_salary_head.salary_head_sequence+200)AS salary_head_id
,m_salary_head.salary_head_name
,t_salary_detail.head_amount
,m_employee.employee_id
,CONCAT(m_employee.employee_first_name,' ',m_employee.employee_last_name)AS Emp_name
FROM m_employee,t_salary_detail,m_salary_head,t_salary_master
WHERE m_employee.employee_id=t_salary_detail.EMPLOYEE_ID
AND t_salary_detail.salary_head_id=m_salary_head.salary_head_id
AND t_salary_master.salary_tran_auto_no=t_salary_detail.salary_tran_auto_no
AND t_salary_master.salary_month=$P{SingleMandatoryMonthPrompt} AND t_salary_master.salary_year=$P{YearPrompt}
AND m_employee.deleted_flag=0 AND m_salary_head.deleted_flag=0
AND t_salary_master.deleted_flag=0 AND t_salary_detail.deleted_flag=0
UNION ALL
SELECT 2 AS sno
,IF(m_salary_head.head_type='E',m_salary_head.salary_head_sequence+100,m_salary_head.salary_head_sequence+200)AS salary_head_id
,IF(m_salary_head.head_type='E',CONCAT(m_salary_head.salary_head_name,' payable'),CONCAT(m_salary_head.salary_head_name,' deduction'))AS salary_head_name
,t_salary_detail.head_amount_payable
,m_employee.employee_id
,CONCAT(m_employee.employee_first_name,' ',m_employee.employee_last_name)AS Emp_name
FROM m_employee,t_salary_detail,m_salary_head,t_salary_master
WHERE m_employee.employee_id=t_salary_detail.EMPLOYEE_ID
AND t_salary_detail.salary_head_id=m_salary_head.salary_head_id
AND t_salary_master.salary_tran_auto_no=t_salary_detail.salary_tran_auto_no
AND t_salary_master.salary_month=$P{SingleMandatoryMonthPrompt} AND t_salary_master.salary_year=$P{YearPrompt}
AND m_employee.deleted_flag=0 AND m_salary_head.deleted_flag=0
AND t_salary_master.deleted_flag=0 AND t_salary_detail.deleted_flag=0
UNION ALL
SELECT 3 AS sno,124 AS salary_head_id,'Monthly Salary'AS salary_head_name
,t_salary_attandance.monthly_salary AS head_amount,m_employee.employee_id
,CONCAT(m_employee.employee_first_name,' ',m_employee.employee_last_name)AS Emp_name
FROM t_salary_attandance,m_employee,t_salary_master
WHERE t_salary_attandance.employee_id=m_employee.employee_id
AND t_salary_master.salary_tran_auto_no=t_salary_attandance.salary_tran_auto_no
AND t_salary_master.salary_month=$P{SingleMandatoryMonthPrompt} AND t_salary_master.salary_year=$P{YearPrompt}
AND m_employee.deleted_flag=0 AND t_salary_master.deleted_flag=0  AND t_salary_attandance.deleted_flag=0
UNION ALL
SELECT 4 AS sno,125 AS salary_head_id,'Other Allowance'AS salary_head_name
,t_salary_attandance.other_allowance AS head_amount,m_employee.employee_id
,CONCAT(m_employee.employee_first_name,' ',m_employee.employee_last_name)AS Emp_name
FROM t_salary_attandance,m_employee,t_salary_master
WHERE t_salary_attandance.employee_id=m_employee.employee_id
AND t_salary_master.salary_tran_auto_no=t_salary_attandance.salary_tran_auto_no
AND t_salary_master.salary_month=$P{SingleMandatoryMonthPrompt} AND t_salary_master.salary_year=$P{YearPrompt}
AND m_employee.deleted_flag=0 AND t_salary_master.deleted_flag=0  AND t_salary_attandance.deleted_flag=0
UNION ALL
SELECT 5 AS sno,126 AS salary_head_id,'Net Amount'AS salary_head_name
,t_salary_attandance.net_amount AS head_amount,m_employee.employee_id
,CONCAT(m_employee.employee_first_name,' ',m_employee.employee_last_name)AS Emp_name
FROM t_salary_attandance,m_employee,t_salary_master
WHERE t_salary_attandance.employee_id=m_employee.employee_id
AND t_salary_master.salary_tran_auto_no=t_salary_attandance.salary_tran_auto_no
AND t_salary_master.salary_month=$P{SingleMandatoryMonthPrompt} AND t_salary_master.salary_year=$P{YearPrompt}
AND m_employee.deleted_flag=0 AND t_salary_master.deleted_flag=0  AND t_salary_attandance.deleted_flag=0
UNION ALL
SELECT 6 AS sno,127 AS salary_head_id,'PF Flag'AS salary_head_name
,t_salary_attandance.pf_flag AS head_amount,m_employee.employee_id
,CONCAT(m_employee.employee_first_name,' ',m_employee.employee_last_name)AS Emp_name
FROM t_salary_attandance,m_employee,t_salary_master
WHERE t_salary_attandance.employee_id=m_employee.employee_id
AND t_salary_master.salary_tran_auto_no=t_salary_attandance.salary_tran_auto_no
AND t_salary_master.salary_month=$P{SingleMandatoryMonthPrompt} AND t_salary_master.salary_year=$P{YearPrompt}
AND m_employee.deleted_flag=0 AND t_salary_master.deleted_flag=0  AND t_salary_attandance.deleted_flag=0
UNION ALL
SELECT 7 AS sno,128 AS salary_head_id,'Base Salary For PF'AS salary_head_name
,t_salary_attandance.base_salary_for_pf AS head_amount,m_employee.employee_id
,CONCAT(m_employee.employee_first_name,' ',m_employee.employee_last_name)AS Emp_name
FROM t_salary_attandance,m_employee,t_salary_master
WHERE t_salary_attandance.employee_id=m_employee.employee_id
AND t_salary_master.salary_tran_auto_no=t_salary_attandance.salary_tran_auto_no
AND t_salary_master.salary_month=$P{SingleMandatoryMonthPrompt} AND t_salary_master.salary_year=$P{YearPrompt}
AND m_employee.deleted_flag=0 AND t_salary_master.deleted_flag=0  AND t_salary_attandance.deleted_flag=0
UNION ALL
SELECT 8 AS sno,332 AS salary_head_id,'Closing Balance of Advance'AS salary_head_name
,m_advance_amnt.closing_balance AS head_amount,m_employee.employee_id
,CONCAT(m_employee.employee_first_name,' ',m_employee.employee_last_name)AS Emp_name
FROM m_advance_amnt,m_employee
,(SELECT employee_id,MAX(transaction_date)AS T_date
FROM m_advance_amnt WHERE deleted_flag=0
GROUP BY 1)AS max_tran
WHERE m_employee.employee_id=m_advance_amnt.employee_id
AND max_tran.employee_id=m_advance_amnt.employee_id
AND max_tran.T_date=m_advance_amnt.transaction_date
AND m_employee.deleted_flag=0 AND m_advance_amnt.deleted_flag=0
UNION ALL 
SELECT 9 AS sno,331 AS salary_head_id,'Advance Adjustment'AS salary_head_name
,IF(transaction_type='Deduction',CONCAT('-',m_advance_amnt.amount),CONCAT('+',m_advance_amnt.amount)) AS head_amount
,m_employee.employee_id
,CONCAT(m_employee.employee_first_name,' ',m_employee.employee_last_name)AS Emp_name
FROM m_advance_amnt,m_employee
,(SELECT employee_id,MAX(transaction_date)AS T_date
FROM m_advance_amnt WHERE  transaction_date>=(SELECT min_date FROM t_salary_master WHERE 
salary_month=$P{SingleMandatoryMonthPrompt} AND salary_year=$P{YearPrompt})
AND transaction_date<=(SELECT max_date FROM t_salary_master WHERE 
salary_month=$P{SingleMandatoryMonthPrompt} AND salary_year=$P{YearPrompt})
AND deleted_flag=0
GROUP BY 1)AS max_tran
WHERE m_employee.employee_id=m_advance_amnt.employee_id
AND max_tran.employee_id=m_advance_amnt.employee_id
AND max_tran.T_date=m_advance_amnt.transaction_date
AND m_employee.deleted_flag=0 AND m_advance_amnt.deleted_flag=0
UNION ALL 
SELECT 10 AS sno,340 AS salary_head_id,'Employee Component'AS salary_head_name
,t_salary_attandance.employee_component AS head_amount,m_employee.employee_id
,CONCAT(m_employee.employee_first_name,' ',m_employee.employee_last_name)AS Emp_name
FROM t_salary_attandance,m_employee,t_salary_master
WHERE t_salary_attandance.employee_id=m_employee.employee_id
AND t_salary_master.salary_tran_auto_no=t_salary_attandance.salary_tran_auto_no
AND t_salary_master.salary_month=$P{SingleMandatoryMonthPrompt} AND t_salary_master.salary_year=$P{YearPrompt}
AND m_employee.deleted_flag=0 AND t_salary_master.deleted_flag=0  AND t_salary_attandance.deleted_flag=0
UNION ALL
SELECT 11 AS sno,341 AS salary_head_id,'Company component'AS salary_head_name
,t_salary_attandance.employer_component AS head_amount,m_employee.employee_id
,CONCAT(m_employee.employee_first_name,' ',m_employee.employee_last_name)AS Emp_name
FROM t_salary_attandance,m_employee,t_salary_master
WHERE t_salary_attandance.employee_id=m_employee.employee_id
AND t_salary_master.salary_tran_auto_no=t_salary_attandance.salary_tran_auto_no
AND t_salary_master.salary_month=$P{SingleMandatoryMonthPrompt} AND t_salary_master.salary_year=$P{YearPrompt}
AND m_employee.deleted_flag=0 AND t_salary_master.deleted_flag=0  AND t_salary_attandance.deleted_flag=0
UNION ALL
SELECT 12 AS sno,342 AS salary_head_id,'Net Payable'AS salary_head_name
,t_salary_attandance.net_payable AS head_amount,m_employee.employee_id
,CONCAT(m_employee.employee_first_name,' ',m_employee.employee_last_name)AS Emp_name
FROM t_salary_attandance,m_employee,t_salary_master
WHERE t_salary_attandance.employee_id=m_employee.employee_id
AND t_salary_master.salary_tran_auto_no=t_salary_attandance.salary_tran_auto_no
AND t_salary_master.salary_month=$P{SingleMandatoryMonthPrompt} AND t_salary_master.salary_year=$P{YearPrompt}
AND m_employee.deleted_flag=0 AND t_salary_master.deleted_flag=0  AND t_salary_attandance.deleted_flag=0
ORDER BY employee_id,salary_head_id,sno) AS a
LEFT JOIN (SELECT DISTINCT t_salary_leaves.employee_id
,CONCAT(LEFT(t_salary_master.salary_month,3),t_salary_master.salary_year)AS monthYear
,t_salary_leaves.closing_leave_balance,t_salary_leaves.opening_leave_balance
,t_salary_leaves.availed_leave
,t_salary_attandance.absent_days,t_salary_attandance.extra_work_day
,t_salary_attandance.hollyd_weekOff,t_salary_attandance.loss_of_pay
,t_salary_attandance.payable_day,t_salary_attandance.present_days
,t_salary_attandance.working_days,t_salary_attandance.total_day_in_month
,m_employee.employee_code,m_employee.join_date,m_employee.increment_date,m_employee_leaves.allow_days AS opening_cl_balance
,m_masters.NAME AS designation
FROM m_employee,t_salary_attandance,m_masters,m_employee_leaves
,t_salary_leaves,t_salary_detail,t_salary_master
WHERE m_employee.employee_id=t_salary_attandance.employee_id
AND t_salary_leaves.employee_id=m_employee.employee_id
AND m_employee.employee_id=t_salary_detail.EMPLOYEE_ID
AND t_salary_detail.salary_tran_auto_no=t_salary_master.salary_tran_auto_no
AND t_salary_master.salary_tran_auto_no=t_salary_leaves.salary_tran_auto_no
AND t_salary_master.salary_tran_auto_no=t_salary_attandance.salary_tran_auto_no
AND t_salary_master.salary_month=$P{SingleMandatoryMonthPrompt} AND t_salary_master.salary_year=$P{YearPrompt}
AND m_employee.designation_id=m_masters.masters_id AND m_employee.deleted_flag=0 
AND m_employee_leaves.employee_id=m_employee.employee_id 
AND m_employee_leaves.leave_id=t_salary_leaves.leave_id
AND t_salary_master.deleted_flag=0 AND t_salary_attandance.deleted_flag=0 
AND t_salary_leaves.deleted_flag=0 AND t_salary_detail.deleted_flag=0)AS b
ON a.employee_id=b.employee_id]]>
	</queryString>
