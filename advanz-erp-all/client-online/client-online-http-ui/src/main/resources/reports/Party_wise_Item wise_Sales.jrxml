	<queryString>
		<![CDATA[SELECT UPPER(CONCAT(m_party.party_name,' ',city.city_name)) AS Party_City
, UPPER(m_item.item_name) AS item_name, UPPER(pack.NAME) AS pack_name
, UPPER(measure.NAME) AS measure_name
, t_bill_detail.batch_no AS batch_no
, ROUND(SUM(IFNULL(t_bill_detail.quantity,0)),2) AS quantity
, ROUND(IFNULL(t_bill_detail.sales_rate,0),2) AS sales_rate
, ROUND(SUM(IFNULL(t_bill_detail.item_value ,0)),2) AS item_value
, ROUND(SUM(IFNULL(t_bill_detail.excise_amount,0)+IFNULL(t_bill_detail.edu_cess_amount,0)+IFNULL(t_bill_detail.h_edu_cess_amount,0)),2) AS excise_amount
, ROUND(SUM(IFNULL(t_bill_detail.discount_amount,0)),2) AS discount_amount
, ROUND(SUM(IFNULL(t_bill_detail.vat_amount,0)+IFNULL(t_bill_detail.cst_amount,0)),2) AS taxable_amount
, ROUND(SUM(IFNULL(t_bill_detail.net_amount,0)),2) AS net_amount
,IFNULL(m_item.item_length,'') AS item_length
,IFNULL(m_item.item_width,'') AS item_width
,IFNULL(m_item.item_thikness,'') AS item_thikness
,IFNULL(m_item.item_density,'') AS item_density
,gr.NAME AS Grade
FROM m_item, m_item_category AS c, (SELECT * FROM m_masters WHERE m_form_name='Pack Type' AND deleted_flag=0) AS pack
, (SELECT * FROM m_masters WHERE m_form_name='Measurement Unit' AND deleted_flag=0) AS Measure
,(SELECT  masters_id ,NAME FROM m_masters WHERE m_form_name='Item Grade' AND deleted_flag=0) AS gr
, t_bill_detail , t_bill_mast , m_party,m_city AS city
WHERE m_item.deleted_flag=0 AND m_item.pack_type_id = pack.masters_id
AND m_item.item_category_id=c.item_category_id AND c.deleted_flag=0
AND city.city_id=t_bill_mast.city_id AND city.deleted_flag=0
AND m_item.measurement_Unit_id = measure.masters_id AND gr.masters_id=m_item.item_grade_id
AND m_item.item_id = t_bill_detail.item_id AND t_bill_detail.deleted_flag=0
AND t_bill_detail.invoice_number = t_bill_mast.invoice_number AND t_bill_mast.deleted_flag=0
AND t_bill_mast.party_id = m_party.party_id AND m_party.deleted_flag=0
AND (c.ITEM_CATEGORY_NAME = $P{ItemCategoryPrompt} OR 1=$P{ItemCategoryNull_FlagPrompt})
AND(m_item.item_name=$P{ItemNamePrompt} OR 1=$P{ItemNameNull_FlagPrompt})
AND(m_party.party_name=$P{PartyNamePrompt} OR 1=$P{PartyNameNull_FlagPrompt})
AND cast(t_bill_mast.invoice_date as date) >= $P{FromDatePrompt}
AND cast(t_bill_mast.invoice_date as date) <= $P{ToDatePrompt}
AND (m_item.active_status=$P{Item_Status_Prompt} OR 1=$P{Item_StatusNull_FlagPrompt})
GROUP BY 1,2,3,4,5,7
ORDER BY 1,2,4]]>
	</queryString>
